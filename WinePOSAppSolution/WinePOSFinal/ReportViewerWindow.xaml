<Window x:Class="WinePOSFinal.ReportViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wfi="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        xmlns:cr="clr-namespace:CrystalDecisions.Windows.Forms;assembly=CrystalDecisions.Windows.Forms"
        Title="ReportViewerWindow" Height="600" Width="800">
    <Grid>
        <wfi:WindowsFormsHost Name="WindowsFormsHost">
            <cr:CrystalReportViewer x:Name="CrystalReportViewer" DisplayGroupTree="False" Dock="Fill" />
        </wfi:WindowsFormsHost>
    </Grid>
</Window>
